            report["Category"] = ""
            report["Month"] = ""
            report["Income"] = 0
            report["Expenses"] = 0
        elif "Month" in columns:
            report = report_data.copy()
            report["Category"] = ""
            report["Type"] = ""
            report["Income"] = 0
            report["Expenses"] = 0
        else:
            return {}
        return report

    def display_report(self, report):
