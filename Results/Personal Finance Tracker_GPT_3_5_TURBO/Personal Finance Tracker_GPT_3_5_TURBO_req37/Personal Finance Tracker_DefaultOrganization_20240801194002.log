                plt.plot(months, incomes, marker="o", label="Income")
                plt.plot(months, expenses, marker="o", label="Expenses")
                plt.xlabel("Month")
                plt.ylabel("Amount")
                plt.title("Monthly Comparison")
                plt.legend()
                plt.show()
        else:
            messagebox.showerror("Error", "Invalid report data.")
    def lift(self):
        self.master.lift()
        self.master.tkraise()
