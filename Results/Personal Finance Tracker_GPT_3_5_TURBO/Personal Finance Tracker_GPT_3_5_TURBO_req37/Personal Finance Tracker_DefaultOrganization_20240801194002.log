            report["category"] = ""
            report["month"] = ""
            report["income"] = 0
            report["expenses"] = 0
        elif "month" in columns:
            report = report_data.copy()
            report["category"] = ""
            report["type"] = ""
            report["income"] = 0
            report["expenses"] = 0
        else:
            return pd.DataFrame()  # Return an empty DataFrame if report data is not recognized
        return report

    def display_report(self, report):
