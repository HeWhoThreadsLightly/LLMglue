        self.is_running = False
        self.timer.cancel()
        self.play_notification("Session paused!")
    def stop(self):
        if not self.is_running:
            return
        self.is_running = False
        self.timer.cancel()
        self.remaining_time = self.work_duration
        self.play_notification("Session stopped!")
