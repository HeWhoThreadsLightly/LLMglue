        print(message)
        # Replace with code to play audio notification
        try:
            pygame.mixer.init()
            pygame.mixer.music.load(self.audio_notification_sound)
            pygame.mixer.music.play()
        except ImportError:
            print("Error: pygame library not found. Audio notification will not be played.")
