            if result:
                try:
                    os.remove(selected_file)
                    messagebox.showinfo("Delete File", "File deleted successfully.")
                except OSError:
                    messagebox.showerror("Delete File", "Failed to delete file.")
        else:
            messagebox.showwarning("Delete File", "Please select a file first.")
