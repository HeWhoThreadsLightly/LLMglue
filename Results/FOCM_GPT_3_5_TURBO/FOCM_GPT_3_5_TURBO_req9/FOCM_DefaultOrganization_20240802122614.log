        self.create_toolbar()
        self.create_status_bar()
    def create_directory_tree(self):
        '''
        Create the directory tree pane in the main window
        '''
        self.directory_tree = Treeview(self)
        self.directory_tree.pack(side=tk.LEFT, fill=tk.BOTH, expand=True)
