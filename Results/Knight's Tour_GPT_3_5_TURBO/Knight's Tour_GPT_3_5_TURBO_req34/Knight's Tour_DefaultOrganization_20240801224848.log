        self.move_counter += 1
        self.game.move_knight(row, col)
        self.update_move_counter()
    def undo_move(self):
        if self.is_paused or self.is_completed:
            return
        self.move_counter -= 1
        self.game.undo()
        self.update_move_counter()
    def show_hint(self):
        if self.is_paused or self.is_completed:
            return
        self.game.show_hint()
