        for row in range(self.grid_size):
            row_buttons = []
            for column in range(self.grid_size):
                index = row * self.grid_size + column
                card = self.cards[index]
                card.row = row
                card.column = column
                button = tk.Button(self.root, text="?", command=lambda c=card: self.flip_card(c))
