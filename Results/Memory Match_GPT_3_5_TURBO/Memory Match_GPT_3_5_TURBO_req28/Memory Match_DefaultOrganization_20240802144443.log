import tkinter as tk
import random
from card import Card
class Game:
    def __init__(self, difficulty="easy"):
        self.difficulty = difficulty
        self.grid_size = self.get_grid_size()
        self.cards = []
        self.face_up_cards = []
        self.create_cards()
        self.create_grid()
