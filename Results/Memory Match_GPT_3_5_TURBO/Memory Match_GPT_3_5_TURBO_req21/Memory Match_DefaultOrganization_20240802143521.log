            card2 = Card(value)
            self.cards.append(card1)
            self.cards.append(card2)
    def create_grid(self):
        # Create a grid of buttons representing the cards
        self.grid = []
        for row in range(self.grid_size):
            row_buttons = []
            for column in range(self.grid_size):
